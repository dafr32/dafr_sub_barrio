{
  // Located at /modules/contrib/blazy_photoswipe, run:
  // eslint . -o ../../eslint/blazy_photoswipe.html -f html
  "extends": "../../../core/.eslintrc.json",
  "ignorePatterns": ["*.yml", "js/*/blazy.photoswipe.js", "config/**/*", "tests/*", "js/*.min.js", "js/*/*.min.js", "js/*/*/*.min.js"],
  "globals": {
    "Blazy": true,
    "dBlazy": true,
    "PhotoSwipeLightbox": true,
    "PhotoSwipe": true,
    "PhotoSwipeUI_Default": true
  },
  // "parser": "babel-eslint",
  "parserOptions": {
    // Required for certain syntax usages.
    "ecmaVersion": 2020
  },
  "rules": {
    "consistent-this": [0, "me"],
    "max-params": 0,
    "id-length": [1, {
      "min": 1
    }],
    "object-shorthand": [2, "consistent"],
    "max-nested-callbacks": [1, 4],
    "prettier/prettier": 0
  },
  "overrides": [{
    "files": ["js/src/blazy.photoswipe5.js"]
  }]
}
